            <gco:CharacterString>Retail trade sales per capita, by region (2012 - 2017)</gco:CharacterString>
          </gmd:title>
          <gmd:presentationForm>
            <gmd:CI_PresentationFormCode codeList="./resources/codeList.xml#CI_PresentationFormCode" codeListValue="005"/>
          </gmd:presentationForm>
        </gmd:CI_Citation>
